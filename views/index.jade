extends layout

block content
  h1= title
  .input
   form(method="post")
    input(name="method")
    input(name="url")
    button Send
  .output
   if (response)
    p Response Status #{response.status} #{response.statusText}
    p= response.data
    
    .headers
    h2 Headers
    each header,key in response.headers
     p= key+":"+header
    
    